<Window x:Class="_20221205.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:_20221205"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="600">
  <Grid>
    <Canvas Name="MyCanvas">
      <local:TT3 x:Name="MyTT3"
                 MyText="TestText"
                 Foreground="MediumPurple"
                 FontSize="50"
                 DragDelta="TT3_DragDelta"
                 Canvas.Left="10" Canvas.Top="10"/>
      <Ellipse Width="100" Height="100" Fill="Pink" Canvas.Left="200" Canvas.Top="100"/>
      <local:BB Width="100" Height="100" Canvas.Left="350" Canvas.Top="100"/>
      <Button Name="Button1" Content="Button1" Click="Button1_Click" Canvas.Left="200"/>
      <local:AAA x:Name="MyAAA" MyText="AAAXAML" FontSize="50" Canvas.Top="100"/>
      <local:BBB x:Name="MyBBB" MyText="BBBXAML" FontSize="50" Canvas.Top="50"/>
    </Canvas>
  </Grid>
</Window>
