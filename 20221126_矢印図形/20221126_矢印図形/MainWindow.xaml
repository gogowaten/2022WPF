<Window x:Class="_20221126_矢印図形.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:_20221126_矢印図形"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="600">
    <Grid>
    <local:Arrow X1="10" Y1="10" X2="20" Y2="100" HeadSize="10"
                 Fill="Purple" StrokeThickness="3" Stroke="Red"/>
    <Canvas>
      <local:Arrow X1="100" Y1="100" X2="200" Y2="100" HeadSize="10"
                   Fill="Purple" StrokeThickness="3" Stroke="Red"/>
      
    </Canvas>
    </Grid>
</Window>
