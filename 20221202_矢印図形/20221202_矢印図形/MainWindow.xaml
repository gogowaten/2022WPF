<Window x:Class="_20221202_矢印図形.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:_20221202_矢印図形"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="600">
    <Grid>
    <Canvas Background="Black">
      <local:Arrow HeadSize="50" Angle="30" HeadType="Type0"
                   X1="10" Y1="50" X2="200" Y2="50"
                   Stroke="Red" StrokeThickness="20"/>
      <local:Arrow HeadSize="50" Angle="30" HeadType="Type1"
                   X1="10" Y1="110" X2="200" Y2="110"
                   Stroke="Red" StrokeThickness="20"/>
      
    </Canvas>
    </Grid>
</Window>
